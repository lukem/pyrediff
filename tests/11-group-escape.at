AT_SETUP([pyre \g<group> escaping])

dnl Create pattern file.
dnl Group <unknown> is not defined, so it is matched literally in the output.
dnl
AT_DATA([11.re], [dnl
r(?P<re>.*)ge(?P=re)x
second
reg\g<re>x
fourth
fifth missing
sixth \g<unknown>
seven(77|47)\g<re>\1\.
])

dnl Create output file.
dnl
AT_DATA([11.out], [dnl
r+*ge+*x
second
reg+*x
fourth
sixth \g<unknown>
seven47+*47.
])

dnl Define PYRE_11 for expected pyre output.
dnl
m4_pushdef([PYRE_11], [dnl
5d4
< fifth missing
])dnl

AX_AT_DIFF_PYRE([11.re], [11.out], [1], [PYRE_11])

AT_CHECK([diff 11.re 11.out | ${abs_top_srcdir}/src/pyrediff -f], [1], [PYRE_11])

AT_CHECK([${abs_top_srcdir}/src/pyrediff 11.re 11.out], [1], [PYRE_11])

m4_popdef([PYRE_11])dnl

AT_CLEANUP()
